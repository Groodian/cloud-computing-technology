[masters]
kubernetes-master ansible_host=10.156.0.5

[workers]
kubernetes-worker-0 ansible_host=10.156.0.3
kubernetes-worker-1 ansible_host=10.156.0.6
kubernetes-worker-2 ansible_host=10.156.0.2

[masters:vars]
ansible_user=cluster
ansible_ssh_private_key_file=../infrastructure/.ssh/cluster
ansible_ssh_common_args='-o ProxyCommand="ssh -o StrictHostKeyChecking=no -oUserKnownHostsFile=/dev/null -o IdentityFile=../infrastructure/.ssh/bastion -W %h:%p bastion@34.159.117.57"'

[workers:vars]
ansible_user=cluster
ansible_ssh_private_key_file=../infrastructure/.ssh/cluster
ansible_ssh_common_args='-o ProxyCommand="ssh -o StrictHostKeyChecking=no -oUserKnownHostsFile=/dev/null -o IdentityFile=../infrastructure/.ssh/bastion -W %h:%p bastion@34.159.117.57"'
